<style>
    .mySlides {
        display: none;
    }

    .w3-left, .w3-right, .w3-badge {
        cursor: pointer;
    }

    .w3-badge {
        height: 13px;
        width: 13px;
        padding: 0;
    }

    .fancybox-nav span {
        visibility: visible !important;
    }
</style>



<div ng-controller="ParticularPortfolioController" class="portfoliodetail">
    
    <div class="breadcrumb">
        <div class="container">
            <ul>
                <li><a href="./" ui-sref="home">Home</a></li>
                <li><a href="/portfolio" ui-sref="portfolio">Portfolio</a></li>
                <li class="active">{{PortfolioDetail.Title}}</li>
            </ul>
        </div>
    </div>

    <div class="section-mn">
        <div ng-if="PortfolioDetail.BannerImageURL">
            <a href="#" title="{{PortfolioDetail.Title}}">
                <img ng-src="/PortfolioImages/BannerImages/{{PortfolioDetail.BannerImageURL}}" alt="{{PortfolioDetail.BannerImageTooltip}}" title="{{PortfolioDetail.BannerImageTooltip}}" />
            </a>
        </div>
    </div>

    <div ng-if="!PortfolioDetail.objPortfolioLogo.length > 0">
        <section clss="slide" ng-if="PortfolioDetail.objPortfolioImageEntity.length>0 || PortfolioDetail.objPortfolioObjectiveEntity.length > 0 ">
            <div class="container">
                <div class="row">

                    <div class="col-sm-6">
                        <div class="slider_flex">
                            <div ng-if="PortfolioDetail.objPortfolioImageEntity.length>0">

                                <div class="w3-right w3-hover-text-khaki leftArrow" ng-click="plusDivs(1)"><img src="/Images/arrow-left.png" /></div>

                                <ul class="slides port_slid">
                                    <li ng-repeat="image in PortfolioDetail.objPortfolioImageEntity track by $index" class="flex-active-slide">
                                        <a class="popup-link fancybox" href="/PortfolioImages/OtherImages/{{image.ImageURL}}" rel="ligthbox" data-fancybox-group="gallery">
                                            <img ng-src="/PortfolioImages/OtherImages/{{image.ImageURL}}" alt="{{image.SEOTitleTag}}" title="{{image.SEOTitleTag}}" alt="Image" class="mySlides" style="cursor:zoom-in" />
                                        </a>
                                    </li>
                                </ul>

                                <div class="w3-left w3-hover-text-khaki rightArrow" ng-click="plusDivs(-1)"><img src="/Images/arrow-right.png" /></div>
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-6" ng-if="PortfolioDetail.objPortfolioObjectiveEntity.length > 0">
                        <div class="title">
                            <h4 class="text-black text-left">Objective</h4>
                        </div>
                        <ul class="list1">
                            <li ng-repeat="objective in PortfolioDetail.objPortfolioObjectiveEntity track by $index">
                                <span ng-bind="objective.ObjectiveText"></span>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </section>
    </div>


    <section clss="main-features" ng-if="PortfolioDetail.objPortfolioFeatureEntity.length > 0">
        <div class="container">
            <div class="title">
                <h4 class="text-black text-left">Main Features</h4>
            </div>
            <ul class="list1">
                <li ng-repeat="feature in PortfolioDetail.objPortfolioFeatureEntity track by $index">
                    <span ng-bind="feature.FeatureText"></span>
                </li>
            </ul>
        </div>
    </section>

    <section clss="section1 solution" ng-if="PortfolioDetail.objPortfolioSolutionEntity.length > 0">
        <div class="container">
            <div class="title">
                <h4 class="text-black text-left">Solution</h4>
            </div>
            <ul class="list1">
                <li ng-repeat="solution in PortfolioDetail.objPortfolioSolutionEntity track by $index">
                    <span ng-bind="solution.SolutionText"></span>
                </li>
                <li ng-if="PortfolioDetail.Title == 'My Class Campus'">
                    For more details: <a href="http://myclasscampus.com/" target="_balnk"> <u style="color: #F58634;"><i>click here</i></u></a>
                </li>
            </ul>
        </div>
    </section>

    <section clss="section1 result-obtained" ng-if="PortfolioDetail.objPortfolioResultEntity.length > 0">
        <div class="container">
            <div class="title">
                <h4 class="text-black">Result Obtained</h4>
            </div>
            <ul class="list1">
                <li ng-repeat="result in PortfolioDetail.objPortfolioResultEntity track by $index">
                    <span ng-bind="result.ResultText"></span>
                </li>
            </ul>
        </div>
    </section>

    <section clss="section1 technology"  ng-if="PortfolioDetail.objPortfolioTechnologyEntity.length > 0">
        <div class="container">
            <div class="title">
                <h4 class="text-black">Technology</h4>
            </div>
            <ul class="list1">
                <li ng-repeat="technology in PortfolioDetail.objPortfolioTechnologyEntity track by $index">
                    <span ng-bind="technology.TechnologyText"></span>
                </li>
            </ul>
        </div>
    </section>

    <section clss="section1 challenges-faced" ng-if="PortfolioDetail.objChallengeFaced.length > 0">
        <div class="container">
            <div class="title">
                <h4 class="text-black">Challenges Faced</h4>
            </div>
            <ul class="list1">
                <li ng-repeat="challenge in PortfolioDetail.objChallengeFaced track by $index">
                    <span ng-bind="challenge.ChallengeFaced"></span>
                </li>
            </ul>
        </div>
    </section>


    <section class="port" ng-if="PortfolioDetail.objPortfolioLogo.length > 0">
        <div class="container">
            <div class="row">
                <div class="col-sm-4" ng-repeat="image in PortfolioDetail.objPortfolioLogo track by $index">
                    <div class="logo-portfolio">
                        <a class="popup-link fancybox design-image" href="/PortfolioImages/OtherImages/{{image.ImageURL}}" rel="ligthbox" data-fancybox-group="gallery">
                            <img ng-src="/PortfolioImages/OtherImages/{{image.ImageURL}}" alt="{{image.SEOTitleTag}}" title="{{image.SEOTitleTag}}" style="cursor:zoom-in" />
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </section>



    <section class="related-post">
        <div class="container">
            <div class="title">
                <h4 class="text-black">Related Post</h4>
            </div>
            <div id="isotope-container" class="isotope">
                <div ng-if="PortfolioDetail" ng-controller="PortfolioController">
                    <div ng-repeat="pl in PorfolioList" class="isotope-element isotope-element-3 grid-item">
                        <a ng-click="RedirectToProtfolioDetail(pl.TitleSeoUrl, pl.SEOTitleTag)">
                            <figure>
                                <div class="portfolio-img"><img title="{{pl.FrontImageTooltip}}" ng-src="/PortfolioImages/FrontPageImages/{{pl.FrontPageImageURL}}" alt="{{pl.FrontImageTooltip}}" /></div>
                                <figcaption>
                                    <div class="portfolio-hover-main text-center">
                                        <div class="portfolio-hover-box">
                                            <div class="portfolio-hover-content">
                                                <span ng-bind="pl.Title"></span>
                                            </div>
                                        </div>
                                    </div>
                                </figcaption>
                            </figure>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>